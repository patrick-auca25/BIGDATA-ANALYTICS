                      "spec": {
                        "dataset": "energy",
                        "editorMode": "code",
                        "format": "table",
                        "rawQuery": true,
                        "rawSql": "WITH today AS (\r\n    SELECT \r\n        time_bucket('1 hour', timestamp) as hour,\r\n        SUM(energy) as energy_today\r\n    FROM energy_readings\r\n    WHERE timestamp >= DATE_TRUNC('day', NOW())\r\n    GROUP BY hour\r\n),\r\nyesterday AS (\r\n    SELECT \r\n        time_bucket('1 hour', timestamp) as hour,\r\n        SUM(energy) as energy_yesterday\r\n    FROM energy_readings\r\n    WHERE timestamp >= DATE_TRUNC('day', NOW()) - INTERVAL '1 day'\r\n      AND timestamp < DATE_TRUNC('day', NOW())\r\n    GROUP BY hour\r\n)\r\nSELECT \r\n    today.hour as time,\r\n    today.energy_today as \"Today\",\r\n    yesterday.energy_yesterday as \"Yesterday\"\r\nFROM today\r\nLEFT JOIN yesterday ON EXTRACT(HOUR FROM today.hour) = EXTRACT(HOUR FROM yesterday.hour)\r\nORDER BY today.hour;",
                        "sql": {
